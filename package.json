{
  "name": "@kickstartds/storyblok-starter-ds-agency",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "BUILD_ENV=preview next dev",
    "build": "next build && next export",
    "build-preview": "BUILD_ENV=preview yarn build",
    "start": "next start",
    "lint": "next lint",
    "ssl-proxy": "local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "converter": "esbuild helpers/createStoryblokConfig.ts --outfile=helpers/createStoryblokConfig.js --bundle --platform=node --target=node16 && node helpers/createStoryblokConfig.js",
    "pull-content-schema": "source ./.env.local && storyblok pull-components --space $STORYBLOK_SPACE_ID -p types/ && mv types/components.$STORYBLOK_SPACE_ID.json types/components-schema.json",
    "generate-content-types": "source ./.env.local && npm run pull-content-schema && storyblok-generate-ts source=./types/components-schema.json target=./types/components-schema",
    "push-components": "source ./.env.local && storyblok push-components --space $STORYBLOK_SPACE_ID storyblok/components.123456.json",
    "create-storyblok-config": "kickstartDS cms storyblok --components-path node_modules/@kickstartds/ds-agency/dist/components --configuration-path storyblok",
    "storyblok-login": "storyblok logout && storyblok login"
  },
  "dependencies": {
    "@kickstartds/base": "^3.0.0-beta.0",
    "@kickstartds/blog": "^3.0.0-beta.0",
    "@kickstartds/content": "^3.0.0-next.6",
    "@kickstartds/core": "^3.0.0-beta.0",
    "@kickstartds/ds-agency": "2.3.0--canary.44.1497.0",
    "@kickstartds/form": "^3.0.0-beta.0",
    "@storyblok/react": "^2.4.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storyblok-rich-text-react-renderer": "^2.9.0"
  },
  "devDependencies": {
    "@kickstartds/jsonschema-utils": "^2.11.1",
    "@kickstartds/jsonschema2netlifycms": "^2.8.1",
    "@kickstartds/jsonschema2stackbit": "^1.2.1",
    "@kickstartds/jsonschema2storyblok": "^1.3.1",
    "@kickstartds/jsonschema2types": "^1.1.23",
    "@kickstartds/jsonschema2uniform": "^1.3.1",
    "@kickstartds/style-dictionary": "^2.2.1",
    "@types/node": "20.4.5",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "esbuild": "^0.19.5",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "import-meta-resolve": "~3.0.0",
    "json-schema-typed": "~8.0.1",
    "kickstartds": "2.2.0--canary.44.259.0",
    "local-ssl-proxy": "^2.0.5",
    "next": "13.4.12",
    "storyblok": "^3.10.1",
    "storyblok-generate-ts": "^1.15.0",
    "typescript": "5.1.6"
  }
}
